package PExa7;
 
import PExa7.*;
import java.util.*;

public class Main {

	public static void main(String[] args) {
		try {
			CuentaBancaria Cb = new CuentaBancaria("Pepe","12345678A","1234-5678-90-1234567890");
			System.out.print(Cb);
		}catch (IllegalArgumentException Iae) {
			System.out.print(Iae.getMessage());
		}
	}
	
	 public static String getInfoCliente(CuentaBancaria cuenta) {
	        return "Nombre: " + cuenta.getNombre() + "\nDNI: " + cuenta.getDni() + "\nCCC: " + cuenta.getCCC();
	    }
}
