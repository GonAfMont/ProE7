package PExa7;
 
import PExa7.*;
import java.util.*;

public class Main {

	public static void main(String[] args) {
		try {
			
			CuentaBancaria Cb = new CuentaBancaria("Pepe Pérez","12345678A",1234,5678,90,1234567890);
			
			
			boolean console = true;
			switch (console) {
			case 1: System.out.print("CCC completo" + Cb.getCCC());
			case 2: System.out.print("Nombre del titular:" + Cb.getNombre());
			case 3: System.out.print("Código de la Entidad Bancaria:" + Cb.getnEntidad());
			case 4: System.out.print("Código de la Sucursal:" + Cb.getnSucursal());
			case 5: System.out.print("Número de Cuenta:" + Cb.getcCuenta());
			case 6: System.out.print("");
			case 7: System.out.print("");
			case 8: System.out.print("");
			case 9: System.out.print("");
			case 10: console = false;
			
			}
			
		}catch (IllegalArgumentException Iae) {
			System.out.print(Iae.getMessage());
		}
	}
}
